{
	"name" : "coka/doctrine-secret-type",
	"type" : "library",
	"license" : "MIT",
	"description" : "Custom Doctrine type that encrypted column value with openssl.",
	"keywords" : [
		"Doctrine Type secret OPENSSL"
	],
	"authors" : [{
			"name" : "Cedrick Oka Baidai",
			"email" : "okacedrick@gmail.com"
		}
	],
	"require" : {
		"php" : ">=7.2",
		"doctrine/dbal" : "^2.9"
	},
	"require-dev" : {
		"symfony/phpunit-bridge" : "^5.0"
	},
	"autoload" : {
		"psr-4" : {
			"Oka\\Doctrine\\" : "src"
		},
		"exclude-from-classmap" : [
			"/tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Oka\\Doctrine\\Tests\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.x.x-dev"
		},
		"symfony" : {
			"allow-contrib" : "true"
		}
	},
	"support" : {
		"docs" : "https://github.com/CedrickOka/doctrine-secret-type/blob/master/src/Resources/doc/index.md"
	}
}
